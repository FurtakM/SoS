On SailEvent(event) do
begin
case event of
     eventCenterCamOnHeike:
        begin
        repeat
         wait(0$1);
         CenterOnUnits(Heike);
        until IsDying(Heike);
        end;
     end;
End;

On UnitDestroyed(un) do
begin
if un = Heike and hasNormalScene then
   YouLost('Heike');
End;