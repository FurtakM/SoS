// Use this on map when u don't need custom logic
// Otherwise, copy and override it on your map script
On CustomCommand(p1, p2, p3, p4, p5, p6) do
begin
if p2 = 102 then
   begin
   lockerBusy := false;
   lockerCodeValid := p3;
   end
else
   SOS_CustomCommand(p1, p2, p3, p4, p5, p6);
End;

On MapStart(p) do
begin
SOS_MapStart();
End;