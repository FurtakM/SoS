// Events module 
On UnitDestroyed(un) do
begin
if un = Heike then
   YouLost('Heike');

if un = Aviradze then
   YouLost('Aviradze');

if un = usLab then
   YouLost('Lab');
End;

On UnitDestroyed_Extra(un, killerSide, killerUnit) do
begin
if un in usSci and killerSide = 2 then
   sciCounterKill := sciCounterKill + 1;
End;

On Contact(s1, s2) do
begin
if s1 = 2 and s2 = 5 then
   YouLost('FriendlyFire');
End;