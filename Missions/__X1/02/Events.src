// Events module 
On UnitDestroyed(un) do
begin
if un = Heike then
   YouLost('Heike');

if un = Aviradze then
   YouLost('Aviradze');

if un in usSci then
   sciCounterKill := sciCounterKill + 1;

if un = usLab then
   YouLost('Lab');
End;

On Contact(s1, s2) do
begin
if s1 = 2 and s2 = 5 then
   YouLost('FriendlyFire');
End;