Starting
begin
ResetFog;
AnimateTrees(true);
RandomizeAll;
Init;

PrepareNature;
PrepareAmericans;
PrepareArabian;

if debug then
   DebugMode;

Action;
End;

Export debug, prefix, missionStart, aviradzeSpotted, deltaSpotted,
       sciSpotted, aviradzeQuestDone, sciCounterKill, usAlert, usActiveDefend,
       timeUntilFarmer, attackDeltaAllowed;

Function Init;
begin
debug := true;
missionStart := false;
aviradzeSpotted := false;
deltaSpotted := false;
sciSpotted := false;
sciCounterKill := 0;
aviradzeQuestDone := false;
attackDeltaAllowed := false;
usAlert := false;
usActiveDefend := false;
timeUntilFarmer := [40$00, 30$00, 20$00][Difficulty];
prefix := '01';
End;

Function DebugMode;
begin
FogOff(1);
Difficulty := 2;
End;