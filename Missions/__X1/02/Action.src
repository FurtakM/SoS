Export Function Action;
begin
InGameOn;

ComMoveXY(Heike, 81, 65);

if Mike then
   ComMoveXY(Mike, 81, 68);

if Givi then
   ComMoveXY(Givi, 85, 72);

if Kamil then
   ComMoveXY(Kamil, 83, 73);

CenterNowOnUnits(Heike);

if Givi or Mike or Kamil then
   begin
   if Givi then
      begin
      AddComTurnUnit(Givi, Heike);
      AddComTurnUnit(Heike, Givi);
         
      Say(Givi, 'DG-1-start');
      end;

   Say(Heike, 'DH-1-start');

   if Mike then
      begin
      AddComTurnUnit(Mike, Heike);
      AddComTurnUnit(Heike, Mike);

      Say(Mike, 'DM-1-start');
      end;

   if Givi or Mike then
      Say(Heike, 'DH-2-start');

   Say(Mike, 'DM-2-start');
   Say(Givi, 'DG-2-start');
   Say(Mike, 'DM-3-start');
   Say(Heike, 'DH-3-start');
   end
else
   begin

   end;

InGameOff;

ChangeMissionObjectives('OEast');
End;
