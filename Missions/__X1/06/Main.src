Export debug, prefix;
Function Init;
begin
debug := true;
prefix := '05_';
End;

Export Function GetSelected(side);
var i, tmp;
begin
if not side then
   exit;

tmp := FilterAllUnits([f_side, side]);

if not tmp or not IsSelected(tmp) then
   exit;

result := [];

for i in tmp do
    if IsSelected(i) then
       result := Replace(result, result + 1, i);
End;