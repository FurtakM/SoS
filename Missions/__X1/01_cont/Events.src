On UnitDestroyed(un) do
begin
if un = Heike then
   YouLost('Heike');

if un in russianPatrol then
   begin
   russianPatrol := russianPatrol diff un;
   dogTag := Replace(dogTag, dogTag + 1, [GetX(un), GetY(un)]);
   end;
End;

On UnitGoesToRed(un) do
begin
if un = Heike then
   YouLost('Heike');
End;