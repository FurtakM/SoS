Every 0$40 + 0$40 trigger GetResourceArea(cratesRiverArea, mat_cans) < [300, 150, 100, 80][Difficulty] do
begin
enable;
CreateCratesArea(rand(1, 5), cratesRiverArea, true);
End;

Every 2$00 trigger GetResourceArea(cratesForestArea, mat_cans) < [300, 250, 200, 180][Difficulty] do
begin
enable;
CreateCratesArea(rand(1, 5), cratesForestArea, true);
End;

Every 0$40 trigger tick < 4$10 do
begin
enable;
CreateCratesXYR(rand(1, 5), 130, 18, 12, true);
End;

