On Contact(side1, side2) do
begin

     If side1 = GetSide(Louis) then

        If side2 = 5 or side2 = 8 then
        begin
        Wait(0$0.3);
        YouLost('YouAttack');
        end
        else
        exit;

end;


Every 0$01 trigger GetLives(Louis) <= 499 do
begin

  InGameOn;
  CenterNowOnUnits(Louis);

  SetLives(Louis, 200);
  Wait(0$1.3);
  SetLives(Louis, 1);
 

  Wait(0$0.5);
  YouLost('Dead');

end;
