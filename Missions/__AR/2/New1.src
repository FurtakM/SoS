Export Function PointBetweenXY(x1, y1, x2, y2);
var _x, _y;
begin
_x := (x1 + x2) div 2;

if _x < 0 then
   _x := _x * -1;

_y := (y1 + y2) div 2;

if _y < 0 then
   _y := _y * -1;

result := [_x, _y];
End;

