Export Louis;
Export Function PrepareLouis;
begin
uc_side := 2;
uc_nation := nation_arabian;

Louis := CreateCharacter('Louis');

if Louis = false then
   begin
   PrepareHuman(sex_male, 1, 6);
   hc_name := 'Louis Tester';
   hc_gallery := '';
   Louis := CreateHuman;
   end;

PlaceHumanInUnit(Louis, ar_depot1);
CenterNowOnUnits(ar_depot1);
End;