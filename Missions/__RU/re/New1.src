On VehicleCaptured(new, old, side, capturing_unit) do
begin
display_strings := [new, old, side, capturing_unit];
End;

Every 0$01 do
begin
enable;
display_strings := GetTaskList(b1);
End;

Export Function Build(units, btype, x, y, dir);
var i;
begin
if not units then
   exit;

for i in units do
    SetTaskList(i, [['B', x, y, 0, btype-1, dir, 0]]);
End;

Every 1 do
Build([a1, a2], b_lab, 29, 40, 0);

Every 0$1 do
SetTaskList(b1, [['p', 20, 19, 0, 1, 0, 0]]);







