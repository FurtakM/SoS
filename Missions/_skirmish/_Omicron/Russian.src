             for p = 1 to h_dmgunit do
                 if GetDistUnits(h_dmgunit[p], b_labb) > 6 then
                    ComMoveXY(h_dmgunit[p], 95, 72);

             if ru2_sci > 0 then
             for i = 1 to ru2_sci do
