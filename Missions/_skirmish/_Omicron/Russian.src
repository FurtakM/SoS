             b_labb := FilterAllUnits([[f_side, 3], [f_or, [f_btype, b_lab], [f_btype, b_lab_half], [f_btype, b_lab_full]]])[1];


       // tablice
          b_weap := AvailableWeaponList(b_fac) diff [ru_crane, ru_cargo_bay, ru_bulldozer, ru_radar, 89];
