                    if not HasTask(ru2_sci[i]) then
                       if GetDistUnits(ru2_sci[i], h_dmgunit[1]) <= 15 then
                          ComHeal(ru2_sci[i], h_dmgunit[1]);

             end;

          // badania etc.
          if h_dmgunit = 0 then
