On UnitDestroyed(un) do
begin

     if un = brave or un = com then
        begin
        SayRadio(doc, 'DDocWin');
        YouLost('');  
        end;

     if un in FilterAllUnits([f_weapon, 91]) then
        begin
        if Rand(1, 100) <= 70 then
           ArtContamination(GetX(un), GetY(un), 10);
        end;

End;

