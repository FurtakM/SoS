Export function PrepareAnimals(num1, num2, num3, area);
var i;
begin
InitHc_All();

for i := 1 to num1 do
    begin
    PrepareApeman(5);
    PlaceUnitArea(CreateHuman, area, false);
    end;

for i := 1 to num2 do
    begin
    PrepareTiger(5);
    PlaceUnitArea(CreateHuman, area, false);
    end;

for i := 1 to num3 do
    begin
    PrepareBird();
    PlaceUnitArea(CreateHuman, area, false);
    end;
End;