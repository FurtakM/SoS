Starting
begin
ResetFog;

InitGame;

PrepareLegion;
PrepareAllies;

Action;
End;

Function InitGame;
begin
RandomizeAll;

Difficulty := GetMultiplayerSetting(0);

if not Difficulty then
   Difficulty := 2;

InitVariables;
SetDiplomacy;
End;

