Every 0$10 do
begin
enable;
CreateCratesAnywhere(5, true);
End;

Every 0$1 do
FogOff(7);

On VehicleConstructionStarted(factory, chassis, engine, control, weapon) do
var change, progress;
begin
if GetSide(factory) <> 7 then
   exit;

change := 4;

repeat
 progress := GetWorkingProgress(factory);
 SetWorkingProgress(factory, progress + change);
 wait(0$1);
until progress >= 100;

// your logic here
SetWorkingProgress(factory, 100);
End;