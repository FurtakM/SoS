Starting
begin
ResetFog;
InitGlobalVariables;
Init;

if debug then
   Difficulty := 2
else
   Difficulty := GetMultiplayerSetting(0);

PrepareAlliance;
PrepareRussian;
Action;
End;

Export debug;
Export Function Init;
begin
debug := false;
End;
