Every 1$35 trigger sikorskiInOmicron and GetResourceArea(cratesSpawnArea, mat_cans) < [18, 15, 12][Difficulty] do
begin
enable;

wait(rand(0$10, 0$30));
CreateCratesArea(rand(2, 5), cratesSpawnArea, true);
End;

Every 1$25 trigger GetResourceArea(ruCratesSpawnArea, mat_cans) < 5 do
begin
enable;
CreateCratesArea(rand(2, 5), ruCratesSpawnArea, true);
End;