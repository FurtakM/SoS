Export Function MOBA_Apeman(side, area, skills);
var un;
begin
uc_side := side;
uc_nation := 0;

hc_skills := skills;
hc_gallery := '';
hc_name := '';
hc_class := class_apeman;

un := CreateHuman;
PlaceUnitArea(un, area, false);
End;

