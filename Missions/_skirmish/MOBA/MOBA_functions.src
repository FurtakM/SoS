Export Function MOBA_LightTank(side, area);
var veh;
begin
vc_chassis := us_light_wheeled;
vc_engine := engine_combustion;
vc_control := control_computer;
vc_weapon := us_machine_gun;

veh := CreateVehicle;
PlaceUnitArea(veh, area, false);
End;

