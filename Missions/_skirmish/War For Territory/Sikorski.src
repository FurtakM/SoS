Export Sikorski;
Export Function PrepareSikorski;
var i, j, un;
begin
uc_side := 1;
uc_nation := 1;

hc_importance := 100;
hc_name := 'Jeremy Sikorski';
hc_gallery := 'us';
hc_face_number := 19;

PrepareHuman(sex_male, 1, [4, 4, 3][Difficulty]);

hc_attr := [10, 12];

Sikorski := CreateHuman;

PlaceUnitXYR(Sikorski, 133, 15, 2, false);

InitHc_All();

for i := 1 to [4, 3, 3][Difficulty] do
    for j := 1 to 4 do
        begin
        PrepareHuman(false, j, [5, 4, 3][Difficulty]);
        un := CreateHuman;
        PlaceUnitXYR(un, 133, 15, 6, false);
        end;
End;