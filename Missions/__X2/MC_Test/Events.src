On SailEventCustom(event, p1, p2, p3, p4, p5, p6) do
begin
MCE_SailEventCustom(event, p1, p2, p3, p4, p5, p6);
End;

On ClassChanged(unit, old_class, new_class) do
begin
MCE_ClassChanged(unit, old_class, new_class);
End;

On UnitDestroyed(unit) do
begin
MCE_UnitDestroyed(unit);
End;

On EvacuateVehicle(driver, vehicle, p1, p2) do
begin
MCE_EvacuateVehicle(driver, vehicle, p1, p2);
End;