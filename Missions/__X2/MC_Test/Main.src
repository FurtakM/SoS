Starting
begin
ResetFog;
End;

Every 0$1 do
begin
MC_RaiseSailEvent(1, 200);
MC_RaiseSailEvent(2, 300);
End;

Export Function MC_RaiseSailEvent(base, event);
begin
async;

case base of
1: Fun1;
2: Fun2;
end;

display_strings := display_strings ^ [base, event, tick];
sync;
End;

Function Fun1;
begin
wait(0$10);
End;

Function Fun2;
begin
display_strings := display_strings ^ 'test';
End;