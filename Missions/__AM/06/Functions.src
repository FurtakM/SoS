   SaveVariable(1, 'FrankIn06');
   end
else
   if IsOk(Frank) then
      SaveVariable(1, 'FrankStayInDelta');

if Brown in selectedPeople then
   begin
   SaveCharacters(Brown, mission_prefix&'Brown');
   SaveVariable(1, 'BrownIn06');
   end
else
