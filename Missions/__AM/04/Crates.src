Every 1$15 do
begin

  if supply <= 13 and supply > 7 then   CreateCratesArea(rand(3,5), CratesArea1, true);
  if supply <= 7 and supply > 4 then CreateCratesArea(rand(3,5), CratesArea2, true);
  if supply <= 4 and supply > 0 then CreateCratesArea(rand(3,5), CratesArea3, true); 

  if supply > 0 then
  begin
    supply = supply-1;
    enable;
  end;
end;