On Command(comandid) Do
var i;
begin
if comandid = 65 then
   for i in FilterAllUnits([[f_side, 1]]) do
       ComStop(i);
end;

On UnitDestroyed(un) do
begin
SOS_UnitDestroyed(un);

if un = JMM then
   YouLost('JMM');
End;

On VehicleConstructed(veh, fac) do
begin
SOS_VehicleConstructed(veh, fac);
End;