// Standard targetable event handler
On TargetableSAIL(cmd, unit, selectedUnit, x, y) do
begin
display_strings := [cmd, unit, selectedUnit, x, y];

case cmd of
     200..299: SOS_TargetableSAIL(cmd, unit, selectedUnit, x, y);
     300..399: SOS_TargetableHack(cmd, unit, selectedUnit, x, y);
     end;
End;